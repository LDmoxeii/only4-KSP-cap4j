@startuml
!include ..\lib_eventstorming.puml

Command("LikeArticleComment") [
    点赞文章评论
    --触发者--
    用户
    --属性--
    用户ID
    文章评论ID
    --规则--
    1. 未点赞
]
Command("UnlikeArticleComment") [
    取消点赞文章评论
    --触发者--
    用户
    --属性--
    用户ID
    文章评论ID
    --规则--
    1. 文章评论必须存在
    2. 已点赞
]
@enduml
